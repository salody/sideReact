{
  "name": "enterprise",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint-staged": "lint-staged",
    "dev": "nodemon --watch build --exec \"cross-env NODE_ENV=development webpack-dev-server --config build/webpack.development.js\"",
    "test": "nodemon --watch build --exec \"cross-env NODE_ENV=test webpack-dev-server --config build/webpack.development.js\"",
    "build-prod": "rimraf ./dist && cross-env NODE_ENV=production webpack --config build/webpack.production.js",
    "build-test": "rimraf ./dist && cross-env NODE_ENV=test webpack --config build/webpack.production.js",
    "build:stats": "rimraf ./dist && cross-env NODE_ENV=production webpack --config build/webpack.production.js --json > stats.json"
  },
  "lint-staged": {
    "*.{css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "pre-commit": "lint-staged",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@types/webpack": "^4.4.23",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-json": "^1.3.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "nodemon": "^1.18.9",
    "pre-commit": "^1.2.2",
    "prettier": "^1.15.3",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "purecss": "^1.0.0",
    "vue": "^2.5.22"
  }
}
